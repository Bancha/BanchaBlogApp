    .#{$prefix}menu-item-text-#{$ui},
    .#{$prefix}menu-item-cmp-#{$ui} {
        margin: 0 after($ui-item-text-horizontal-spacing) 0 before($ui-item-text-horizontal-spacing);
    }

    .#{$prefix}menu-item-text-#{$ui} {
